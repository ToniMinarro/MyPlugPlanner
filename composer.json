{
    "name": "my-plug-planner/plug-planner",
    "description": "Plug Planner",
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "doctrine/dbal": "^4.2.3",
        "doctrine/doctrine-bundle": "^2.13.2",
        "doctrine/doctrine-migrations-bundle": "^3.4.1",
        "pccomponentes/ddd": "^4.5",
        "pccomponentes/topic-generator": "^2.0.1",
        "phpdocumentor/reflection-docblock": "^5.6.1",
        "phpstan/phpdoc-parser": "^2.1",
        "symfony/asset": "7.3.*",
        "symfony/asset-mapper": "7.3.*",
        "symfony/console": "7.3.*",
        "symfony/doctrine-messenger": "7.3.*",
        "symfony/dotenv": "7.3.*",
        "symfony/expression-language": "7.3.*",
        "symfony/flex": "^2.5.0",
        "symfony/form": "7.3.*",
        "symfony/framework-bundle": "7.3.*",
        "symfony/http-client": "7.3.*",
        "symfony/intl": "7.3.*",
        "symfony/mailer": "7.3.*",
        "symfony/messenger": "7.3.*",
        "symfony/mime": "7.3.*",
        "symfony/monolog-bundle": "^3.10",
        "symfony/notifier": "7.3.*",
        "symfony/process": "7.3.*",
        "symfony/property-access": "7.3.*",
        "symfony/property-info": "7.3.*",
        "symfony/runtime": "7.3.*",
        "symfony/security-bundle": "7.3.*",
        "symfony/serializer": "7.3.*",
        "symfony/stimulus-bundle": "^2.23",
        "symfony/string": "7.3.*",
        "symfony/translation": "7.3.*",
        "symfony/twig-bundle": "7.3.*",
        "symfony/ux-turbo": "^2.23",
        "symfony/validator": "7.3.*",
        "symfony/web-link": "7.3.*",
        "symfony/yaml": "7.3.*",
        "twig/extra-bundle": "^3.20",
        "twig/twig": "^3"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "phpro/grumphp": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "bump-after-update": true,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Shared\\": "src/Shared/",
            "IberdrolaApi\\": "src/IberdrolaApi/",
            "MyPlugPlanner\\": "src/MyPlugPlanner/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Shared\\Tests\\": "tests/Shared/",
            "IberdrolaApi\\Tests\\": "tests/IberdrolaApi/",
            "MyPlugPlanner\\Tests\\": "tests/MyPlugPlanner/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "importmap:install": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "7.3.*",
            "docker": true
        }
    },
    "require-dev": {
        "behat/behat": "^3.19",
        "pccomponentes/open-api-messaging-context": "^3.3",
        "phpro/grumphp": "^2.11",
        "phpstan/phpstan-symfony": "^2.0.2",
        "phpunit/phpunit": "^12.0.7",
        "rector/rector": "^2.0.10",
        "symfony/browser-kit": "7.3.*",
        "symfony/css-selector": "7.3.*",
        "symfony/debug-bundle": "7.3.*",
        "symfony/maker-bundle": "^1.62.1",
        "symfony/phpunit-bridge": "^7.2",
        "symfony/stopwatch": "7.3.*",
        "symfony/web-profiler-bundle": "7.3.*",
        "symplify/easy-coding-standard": ">=12.5.8"
    }
}
